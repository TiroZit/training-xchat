{
	"name": "practice-shop",
	"version": "1.0",
	"packageManager": "yarn@3.5.0",
	"type": "module",
	"private": true,
	"license": "MIT",
	"engineStrict": true,
	"scripts": {
		"dev": "vite -c config/vite.config.dev.js --host",
		"build": "vite build -c config/vite.config.build.js --emptyOutDir && node ./config/moveFiles.js",
		"deploySFTPHtml": "npm run build && node ./config/deploySFTP.js html",
		"deploySFTPServer": "npm run build && node ./config/deploySFTP.js server"
	},
	"workspaces": [
		"src/*"
	],
	"engines": {
		"node": ">=16"
	},
	"browserslist": [
		">1%",
		"not op_mini all",
		"not ie 11",
		"last 3 chrome version",
		"last 3 firefox version",
		"last 3 safari version"
	],
	"devDependencies": {
		"@babel/core": "^7.21.8",
		"@babel/preset-env": "^7.21.5",
		"@rollup/plugin-babel": "^6.0.3",
		"@rollup/plugin-commonjs": "^25.0.0",
		"@vitejs/plugin-vue": "^4.2.3",
		"@vue/compiler-sfc": "^3.3.2",
		"autoprefixer": "^10.4.14",
		"cssnano": "^6.0.1",
		"postcss": "^8.4.23",
		"postcss-sass": "^0.5.0",
		"postcss-short": "^5.0.0",
		"postcss-sort-media-queries": "^5.1.0",
		"sass": "^1.62.1",
		"ssh2-sftp-client": "^9.1.0",
		"tailwindcss": "^3.3.2",
		"terser": "^5.17.3",
		"unplugin-fonts": "^1.0.3",
		"unplugin-icons": "^0.16.1",
		"unplugin-imagemin": "latest",
		"unplugin-vue-components": "^0.24.1",
		"vite": "^4.3.5"
	},
	"dependencies": {
		"@fontsource/manrope": "^4.5.13",
		"@headlessui/vue": "^1.7.13",
		"@heroicons/vue": "^2.0.18",
		"@iconify/json": "^2.2.65",
		"@tailwindcss/aspect-ratio": "^0.4.2",
		"@tailwindcss/forms": "^0.5.3",
		"@vueform/vueform": "^1.3.2",
		"axios": "^1.4.0",
		"photoswipe": "^5.3.7",
		"pinia": "^2.0.36",
		"swiper": "^9.3.1",
		"vue": "^3.3.2",
		"vue-router": "^4.2.0"
	}
}
